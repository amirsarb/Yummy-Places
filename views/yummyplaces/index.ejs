<% layout('layouts/layout') -%>
<%for (let place of foundAll){%>
<div class="card mb-3">
    <div class="row">
        <div class="col-md-4">
            <img class="img-fluid" alt='' src="<%=place.image%>">
        </div>
        <div class="col-md-8">
            <div class="card-body">
                <h5 class="card-title"><%=place.title %> </h5>
                <p class="card-text"><%=place.description %></p>
                <p class="card-text">
                    <small class="text-muted"><%=place.location%></small>
                </p>
                <a href="/yummyplaces/<%=place._id%>" class="btn btn-primary">View </a> <%=place.titles%>

            </div>
        </div>
    </div>
</div>
<%}%>